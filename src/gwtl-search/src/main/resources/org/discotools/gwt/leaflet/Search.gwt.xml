<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtl_search">
	
	<!-- GWT dependencies -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />

	<!-- Web Technology dependencies -->
<!-- 	<inherits name="org.fusesource.restygwt.RestyGWT" /> -->
	
	<!-- GWT Leaflet dependencies -->
	<inherits name="org.discotools.gwt.leaflet.Core"/>
	
	<!-- Ensure Leaflet search script and styles are pre-loaded -->
	<script     src="libs/leaflet-search/origin/master/3b717c6ff2bbd5ad19ba4f851c152f67ca62b97d/leaflet-search.js" />
	<stylesheet src="libs/leaflet-search/origin/master/3b717c6ff2bbd5ad19ba4f851c152f67ca62b97d/leaflet-search.css" />
	
	<!-- GWT Leaflet source paths -->
	<source path="client"></source>
	
</module>