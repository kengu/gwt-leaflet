<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtl_core">

	<!-- GWT dependencies -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	
	<!-- Ensure Leaflet script and styles are pre-loaded -->
	<script     src="libs/leaflet/origin/master/14a8be5050f24b1d11d8f2e38259880e11c31fdf/leaflet-src.js" />
	<stylesheet src="libs/leaflet/origin/master/14a8be5050f24b1d11d8f2e38259880e11c31fdf/leaflet.css" />
	<stylesheet src="libs/leaflet/origin/master/14a8be5050f24b1d11d8f2e38259880e11c31fdf/leaflet.ie.css" />
	<!-- Ensure Leaflet-search script and styles are pre-loaded -->
	<script     src="libs/leaflet/search/leaflet-search.js" />
	<stylesheet src="libs/leaflet/search/leaflet-search.css" />

	<!-- GWT Leaflet source paths --> 
	<source path="client"></source>

</module>
