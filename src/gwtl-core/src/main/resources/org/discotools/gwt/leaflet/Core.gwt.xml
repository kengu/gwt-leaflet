<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtl_core">

	<!-- GWT dependencies -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	
	<!-- Ensure Leaflet script and styles are pre-loaded -->
	<script     src="libs/leaflet/v0.4.5/leaflet-src.js" />
	<stylesheet src="libs/leaflet/v0.4.5/leaflet.css" />
	<stylesheet src="libs/leaflet/v0.4.5/leaflet.ie.css" />

	<!-- GWT Leaflet source paths --> 
	<source path="client"></source>

</module>
